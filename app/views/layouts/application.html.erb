<!-- Transparent to solid color navbar -->

    <div id="navbar-scroll-start"></div>
      <script type="text/javascript">
      var mainbottom = $('#navbar-scroll-start').offset().top + $('#navbar-scroll-start').height();

      // On scroll
      $(window).on('scroll',function(){

          // Round to reduce a little workload
          stop = Math.round($(window).scrollTop());
          if (stop > mainbottom) {
              $('.navbar-style').addClass('initial-scroll');
          } else {
              $('.navbar-style').removeClass('initial-scroll');
         }
      });
    </script>
  
